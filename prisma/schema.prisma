generator db {
  provider = "go run github.com/prisma/prisma-client-go"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FoodDiary {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  foodName  String
  calories  Float
  date      DateTime
  note      String?
}

model User {
  id             Int     @id
  email          String  @unique
  name           String
  hashedpassword String? @db.VarChar(60)
}
