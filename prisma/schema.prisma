datasource db {
    provider = "sqlite"
    url = "file:ieat.db"
}

generator db {
    provider = "go run github.com/prisma/prisma-client-go"
}

model FoodDiary {
    id          Int @id @default(autoincrement())
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
    foodName    String
    Calories    Float
    Date        DateTime
    Note        String?
}